script/source = "extends RayCast

signal picked_up(obj)
signal released(obj, was_launched)

export(float, 1.0, 8.0, .1) var hold_distance = 2.0
export(float, 1.0, 40.0, .5) var throw_strength = 25.0

var held_obj : Prop = null

var mouse_moved := false
var last_mouse_motion_time = -1

func _ready() -> void:
	Globals.connect(\"unpaused\", self, \"on_unpaused\")
	
func on_unpaused():
	if held_obj:
		release()

func _input(event: InputEvent) -> void:
	if event is InputEventMouseMotion:
		last_mouse_motion_time = Time.get_ticks_msec()
	
	if Input.is_action_just_pressed(\"throw\"):
		if held_obj:
			release(true)
	elif Input.is_action_just_released(\"grab_drop\"):
		Console.write_line(\"grab_drop released\")
		if held_obj:
			release()
	elif Input.is_action_just_pressed(\"grab_drop\"):
		var coll = get_collider() as Prop
		
		if coll and coll.grab_enabled and not held_obj:
			held_obj = coll
			held_obj.picked_up(self, hold_distance)
			emit_signal(\"picked_up\", held_obj)

func release(launch = false):
	if launch:
		held_obj.launched(throw_strength)
	else:
		var now = Time.get_ticks_msec()
		held_obj.dropped((now - last_mouse_motion_time) < 10)
	
	emit_signal(\"released\", held_obj, launch)
	
	held_obj = null

func on_entered_slot():
	emit_signal(\"released\", held_obj, false)
	held_obj = null
"

