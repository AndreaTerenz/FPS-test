script/source = "extends MeshInstance

func _ready() -> void:
	Globals.connect_to_player_set(self, \"_on_player_set\")

func _on_player_set(pl: Player):
	pl.connect(\"env_changed\", self, \"_on_env_changed\")
	visible = (pl.environment == Player.ENVIRONMENT.WATER)
	
func _on_env_changed(env):
	if env == Player.ENVIRONMENT.WATER:
		visible = true
	else:
		visible = false
"

