script/source = "extends RayCast

#there should be a way of computing this from a 'lethal height'
export(float) var min_dmg_speed := 10.0
export(float) var lethal_speed := 80.0

var debug_ignore_dmg := false
var old_pos := Vector3.ZERO
var v_speed := 0.0
var is_falling := true

func _ready() -> void:
	reset()

func reset():
	v_speed = 0
	is_falling = false

func _process(delta: float) -> void:
	if enabled:
		if not is_colliding():
			var new_pos : Vector3 = Utils.get_global_pos(Globals.player)
			if not is_falling:
				is_falling = true
				old_pos = new_pos
			else:
				var v_dist = (new_pos - old_pos).y
				
				if abs(v_dist) > 0.0:
					v_speed = min(abs(v_dist / delta), lethal_speed)
					old_pos = new_pos
				
		elif is_falling and not debug_ignore_dmg:
			var dmg := get_damage()
			Globals.player.hitbox.decrease_hp(dmg)
			reset()
	else:
		reset()

func get_damage() -> float:
	var max_dmg = Globals.player.hitbox_start_hp
	
	var other : CollisionObject = get_collider() as CollisionObject
	if (other and Globals.get_layer_bit_in_object(other, \"Water\")):
		max_dmg = 0.0

	var dmg := range_lerp(v_speed, min_dmg_speed, lethal_speed, 0, max_dmg)
	dmg = clamp(dmg, 0, max_dmg)
	
	return round(dmg)
"

[sub_resource type="GDScript" id=18]
script/source = "extends Spatial

var enabled: bool = true setget set_enabled
var flipped: bool = false

onready var check_top = $\"StairsCheck Top\"
onready var check_mid = $StairsCheck
onready var check_bottom = $\"StairsCheck Bottom\"

func set_enabled(value):
	enabled = value
	check_top.enabled = enabled
	check_mid.enabled = enabled
	check_bottom.enabled = enabled
	if not(enabled):
		flipped = false

func any_collisions() -> bool:
	return check_top.is_colliding() or check_mid.is_colliding() or check_bottom.is_colliding()
	
func top_collision() -> bool:
	return check_top.is_colliding() if not(flipped) else check_bottom.is_colliding()
	
func bottom_collision() -> bool:
	return check_bottom.is_colliding() if not(flipped) else check_top.is_colliding()
	return check_bottom.is_colliding() if not(flipped) else check_top.is_colliding()
"

