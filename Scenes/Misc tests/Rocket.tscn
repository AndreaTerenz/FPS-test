code = "// NOTE: Shader automatically converted from Godot Engine 3.5.beta1's StandardMaterial3D.

shader_type spatial;
render_mode depth_draw_opaque,blend_mix,cull_disabled,ambient_light_disabled;

uniform vec4 albedo : source_color;
uniform sampler2D tint_gradient : hint_default_white;
uniform sampler2D fade_mask : hint_default_white;
uniform sampler2D noise_text : hint_default_white;

void fragment() {
	vec2 base_uv = UV;
	vec3 tint = texture(tint_gradient, base_uv).rgb;
	float noise = texture(noise_text, base_uv + vec2(TIME * 6., 0.)).r;
	
	ALBEDO = tint * noise;
	EMISSION = 5.0 * ALBEDO;
	
	float alpha_val = texture(fade_mask, vec2(base_uv.y, base_uv.x)).r;
	ALPHA = alpha_val + textureLod(fade_mask, vec2(base_uv.y, base_uv.x), .01).r;
}
"

