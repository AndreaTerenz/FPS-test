[sub_resource type="GDScript" id=22]
script/source = "extends RigidBody3D

@export var expl_scene: PackedScene = preload(\"res://Scenes/Elements/Explosion_elem.tscn\")
@export var fuse_length: float = 5.0

@onready var flame = $MeshInstance3D/Flame
@onready var fuse := $Fuse

func _ready() -> void:
	flame.visible = false
	fuse.wait_time = fuse_length

func _input(event: InputEvent) -> void:
	if Input.is_action_just_pressed(\"fire\"):
		flame.visible = true
		fuse.start()
		apply_central_impulse(Utils.local_direction(self, Vector3.FORWARD) * 200.0)

func _on_body_entered(body: Node) -> void:
	detonate()
	
func _on_fuse_timeout() -> void:
	detonate()

func detonate():
	var explosion = expl_scene.instantiate()
	get_parent().add_child(explosion)
	Utils.copy_global_pos(self, explosion)
	explosion.detonate()
	explosion.scale *= 3.0
	
	queue_free()
"

